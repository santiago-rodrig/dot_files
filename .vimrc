execute pathogen#infect()
syntax on
filetype plugin indent on
set nobackup
set noswapfile
set relativenumber
set encoding=UTF-8
set number
set colorcolumn=81
set shiftwidth=2
set tabstop=2
set softtabstop=2
set expandtab
set nowrap
set pastetoggle=<F3>
colorscheme gruvbox
set bg=dark
let mapleader=','
map <C-n> :NERDTreeToggle<CR>
nnoremap <leader>d :tabnext<CR>
nnoremap <leader>s :tabprevious<CR>
